<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <ImageView
        android:layout_width="0dp"
        app:srcCompat="@drawable/party"
        android:id="@+id/imageView3"
        android:scaleType="fitXY"
        android:layout_height="0dp"
        android:adjustViewBounds="false"
        android:cropToPadding="false"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/imageButton2"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:fabSize="mini"
        app:srcCompat="@android:drawable/stat_sys_upload"
        android:id="@+id/floatingActionButton"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/editText5"
        android:layout_marginEnd="22dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="105dp"
        android:layout_marginBottom="102dp"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        android:layout_marginRight="22dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText"
        android:hint="Title"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/imageButton4"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="11dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:inputType="time"
        android:ems="10"
        android:id="@+id/editText4"
        android:hint="Time"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/editText"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/editText"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/editText" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:inputType="date"
        android:ems="10"
        android:id="@+id/editText5"
        android:hint="Date"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/editText4"
        android:layout_marginTop="3dp"
        app:layout_constraintTop_toBottomOf="@+id/editText4"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/editText4" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:inputType="textPostalAddress"
        android:ems="10"
        android:id="@+id/editText6"
        android:hint="Postal Address"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/editText7"
        android:layout_marginStart="1dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/editText5"
        android:layout_marginLeft="1dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:inputType="textMultiLine"
        android:ems="10"
        android:id="@+id/editText7"
        android:hint="Description"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton"
        app:layout_constraintRight_toRightOf="@+id/editText6"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton5"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/editText6" />

    <ImageButton
        android:layout_width="0dp"
        android:layout_height="43dp"
        app:srcCompat="@drawable/common_google_signin_btn_icon_dark_normal"
        android:id="@+id/imageButton"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/imageButton5"
        android:layout_marginTop="13dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton5"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton5" />

    <ImageButton
        android:layout_width="0dp"
        android:layout_height="43dp"
        app:srcCompat="@drawable/common_google_signin_btn_icon_dark_normal"
        android:id="@+id/imageButton5"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/imageButton3"
        android:layout_marginTop="14dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton3"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton3" />

    <ImageButton
        android:layout_width="0dp"
        android:layout_height="43dp"
        app:srcCompat="@drawable/common_google_signin_btn_icon_dark_normal"
        android:id="@+id/imageButton4"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/imageButton2"
        android:layout_marginTop="14dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton2"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton2" />

    <ImageButton
        android:layout_width="0dp"
        android:layout_height="43dp"
        app:srcCompat="@drawable/common_google_signin_btn_icon_dark_normal"
        android:id="@+id/imageButton3"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/imageButton4"
        android:layout_marginTop="13dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton4"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton4" />

    <ImageButton
        android:layout_width="51dp"
        android:layout_height="0dp"
        app:srcCompat="@drawable/common_google_signin_btn_icon_dark_normal"
        android:id="@+id/imageButton2"
        app:layout_constraintRight_toLeftOf="@+id/editText"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="23dp"
        android:layout_marginTop="139dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="23dp" />

    <Button
        android:text="Button"
        android:layout_height="wrap_content"
        android:id="@+id/button5"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        android:layout_width="0dp"
        style="@style/Widget.AppCompat.Button.Colored"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />

</android.support.constraint.ConstraintLayout>